{
  "name": "@hashtree/core",
  "version": "0.1.3",
  "description": "Simple content-addressed merkle tree with KV storage",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./worker": {
      "types": "./dist/worker/protocol.d.ts",
      "import": "./dist/worker/protocol.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "merkle",
    "content-addressed",
    "hash",
    "tree",
    "storage",
    "browser"
  ],
  "author": "Martti Malmi",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://files.iris.to/#/npub1xndmdgymsf4a34rzr7346vp8qcptxf75pjqweh8naa8rklgxpfqqmfjtce/hashtree",
  "homepage": "https://files.iris.to/#/npub1xndmdgymsf4a34rzr7346vp8qcptxf75pjqweh8naa8rklgxpfqqmfjtce/hashtree",
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@noble/hashes": "^2.0.1",
    "@scure/base": "^2.0.0"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "happy-dom": "^15.0.0",
    "playwright": "^1.57.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0",
    "ws": "^8.18.3"
  }
}
