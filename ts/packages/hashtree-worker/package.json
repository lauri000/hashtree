{
  "name": "@hashtree/worker",
  "version": "0.1.0",
  "description": "Modular browser worker for hashtree blob caching and Blossom connectivity",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./p2p": {
      "types": "./dist/p2p/index.d.ts",
      "import": "./dist/p2p/index.js"
    },
    "./entry": {
      "types": "./dist/entry.d.ts",
      "import": "./dist/entry.js"
    },
    "./protocol": {
      "types": "./dist/protocol.d.ts",
      "import": "./dist/protocol.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "hashtree",
    "worker",
    "indexeddb",
    "blossom"
  ],
  "author": "Martti Malmi",
  "license": "MIT",
  "dependencies": {
    "@hashtree/core": "workspace:*",
    "@hashtree/dexie": "workspace:*",
    "@hashtree/nostr": "workspace:*",
    "nostr-tools": "^2.18.2"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
