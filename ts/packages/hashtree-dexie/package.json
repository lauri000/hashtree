  },
  "keywords": [
    "hashtree",
    "dexie",
    "indexeddb",
    "storage"
  ],
  "author": "Martti Malmi",
  "license": "MIT",
  "dependencies": {
    "@hashtree/core": "workspace:*",
    "dexie": "^4.2.1"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
