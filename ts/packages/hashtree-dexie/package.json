{
  "name": "@hashtree/dexie",
  "version": "0.1.3",
  "description": "Dexie-based IndexedDB store for hashtree",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
